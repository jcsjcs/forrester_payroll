<h1>New party_role_type</h1>

<% form_for(@party_role_type) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :name %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_area :description, :cols => 23, :rows => 4 %>
  </p>
  <p>
    <%= f.label :party_type_id %><br />
    <%= f.select :party_type_id, PartyType.find(:all).collect {|p| [ p.name, p.id ] } %>
  </p>
  <p>
    <%= f.label :role_type_id %><br />
    <%= f.select :role_type_id, RoleType.find(:all).collect {|p| [ p.name, p.id ] } %>
  </p>
  <p>
    <%= f.label :created_by %><br />
    <%= f.text_field :created_by %>
  </p>
  <p>
    <%= f.label :valid_from %><br />
    <%= f.date_select :valid_from %>
  </p>
  <p>
    <%= f.label :valid_until %><br />
    <%= f.date_select :valid_until %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', party_role_types_path %>