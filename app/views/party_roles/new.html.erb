<h1>New party_role</h1>

<% form_for(@party_role) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :role_type_id %><br />
    <%= f.select :role_type_id, RoleType.find(:all).collect {|p| [ p.name, p.id ] } %>
  </p>
  <p>
    <%= f.label :party_id %><br />
    <%= f.select :party_id, Party.find(:all).collect {|p| [ p.iparty.name_string, p.id ] } %>
  </p>
  <p>
    <%= f.label :valid_from %><br />
    <%= f.date_select :valid_from %>
  </p>
  <p>
    <%= f.label :valid_until %><br />
    <%= f.date_select :valid_until %>
  </p>
  <p>
    <%= f.label :created_by %><br />
    <%= f.text_field :created_by %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', party_roles_path %>